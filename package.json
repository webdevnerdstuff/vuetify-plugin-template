{
	"name": "vuetify-plugin-template",
	"version": "1.1.0",
	"description": "Vuetify Plugin Template is a template to help you get starting building your own plugin/component for Vuetify.",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/vuetify-plugin-template.cjs.js",
	"module": "dist/vuetify-plugin-template.es.js",
	"types": "dist/plugin/types/index.d.ts",
	"scripts": {
		"dev": "vite",
		"watch": "pnpm dev",
		"play": "sh src/playground/configs/build.sh && NODE_ENV=playground pnpm dev",
		"build": "vue-tsc && npm run test:build && vite build --config vite.build.config.mts",
		"build:docs": "vite build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d docs",
		"prepublishOnly": "npm run build",
		"lint": "eslint src/**/*.{ts,vue}",
		"prepare": "husky install",
		"test:dev": "vitest",
		"test:all": "vitest --run",
		"test:build": "vitest --run --bail 1"
	},
	"lint-staged": {
		"src/**/*.{js,ts,vue}": [
			"npm run lint"
		]
	},
	"author": "WebDevNerdStuff & Bunnies... lots and lots of bunnies! <webdevnerdstuff@gmail.com> (https://webdevnerdstuff.com)",
	"funding": [],
	"license": "MIT",
	"files": [
		"dist/*",
		"LICENSE.md",
		"README.md"
	],
	"repository": "https://github.com/webdevnerdstuff/vuetify-plugin-template",
	"bugs": {
		"url": "https://github.com/webdevnerdstuff/vuetify-plugin-template/issues"
	},
	"homepage": "https://webdevnerdstuff.github.io/vuetify-plugin-template/",
	"keywords": [
		"vuetify-plugin-template",
		"vuetify",
		"vue",
		"vue3",
		"component",
		"plugin",
		"javascript",
		"typescript",
		"webdevnerdstuff",
		"wdns"
	],
	"dependencies": {
		"vue": "^3.5.12",
		"vuetify": "^3.7.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@fortawesome/fontawesome-svg-core": "^6.6.0",
		"@fortawesome/free-brands-svg-icons": "^6.6.0",
		"@fortawesome/free-regular-svg-icons": "^6.6.0",
		"@fortawesome/free-solid-svg-icons": "^6.6.0",
		"@fortawesome/vue-fontawesome": "^3.0.8",
		"@mdi/font": "^7.4.47",
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-node-resolve": "^15.3.0",
		"@rollup/plugin-terser": "^0.4.4",
		"@stylistic/stylelint-plugin": "^3.1.1",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.8.1",
		"@types/prismjs": "^1.26.5",
		"@typescript-eslint/eslint-plugin": "^8.11.0",
		"@typescript-eslint/parser": "^8.11.0",
		"@vitejs/plugin-vue": "^5.1.4",
		"@vue/cli-service": "^5.0.8",
		"@vue/eslint-config-typescript": "^14.1.3",
		"@vue/test-utils": "^2.4.6",
		"@wdns/vue-code-block": "^2.3.3",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-vue": "^9.29.1",
		"gh-pages": "^6.2.0",
		"husky": "^9.1.6",
		"jsdom": "^25.0.1",
		"lint-staged": "^15.2.10",
		"pinia": "^2.2.4",
		"postcss": "^8.4.47",
		"postcss-html": "^1.7.0",
		"postcss-scss": "^4.0.9",
		"prettier": "^3.3.3",
		"prismjs": "^1.29.0",
		"roboto-fontface": "^0.10.0",
		"rollup": "^4.24.0",
		"rollup-plugin-polyfill-node": "^0.13.0",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-scss": "^4.0.0",
		"rollup-plugin-typescript2": "^0.36.0",
		"sass": "^1.80.4",
		"stylelint": "^16.10.0",
		"stylelint-config-standard": "^36.0.1",
		"stylelint-order": "^6.0.4",
		"stylelint-scss": "^6.8.1",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.11.0",
		"unplugin-auto-import": "^0.18.3",
		"vite": "^5.4.10",
		"vite-plugin-css-injected-by-js": "^3.5.2",
		"vite-plugin-dts": "^4.3.0",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-static-copy": "^2.0.0",
		"vite-plugin-stylelint": "6.0.0-beta.1",
		"vite-plugin-vuetify": "^2.0.4",
		"vitest": "^2.1.3",
		"vue-tsc": "^2.1.8",
		"webfontloader": "^1.6.28"
	}
}
